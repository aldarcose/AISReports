<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IHubService" />
            </basicHttpBinding>
            <customBinding>
                <binding name="Soap12_HubService">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://demo-hub.zdrav.netrika.ru/Service/HubService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IHubService"
                contract="ServiceReference.IHubService" name="BasicHttpBinding_IHubService" />
            <endpoint address="http://demo-hub.zdrav.netrika.ru/Service/HubService.svc/soap12"
                binding="customBinding" bindingConfiguration="Soap12_HubService"
                contract="ServiceReference.IHubService" name="Soap12_HubService" />
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
